<p id="notice"><%= notice %></p>

<h1>Listing Download Excels</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Fristname</th>
      <th>Lastname</th>
      <th>Age</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @download_excels.each do |download_excel| %>
      <tr>
        <td><%= download_excel.name %></td>
        <td><%= download_excel.fristname %></td>
        <td><%= download_excel.lastname %></td>
        <td><%= download_excel.age %></td>
        <td><%= link_to 'Show', download_excel %></td>
        <td><%= link_to 'Edit', edit_download_excel_path(download_excel) %></td>
        <td><%= link_to 'Destroy', download_excel, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Download Excel', new_download_excel_path %> | <%= link_to 'Download Excel', download_excels_path(:format => "xls") %>
